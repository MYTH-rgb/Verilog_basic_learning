    %load/vec4 v0x5c03859238e0_0;
    %pad/u 5;
    %load/vec4 v0x5c03859239d0_0;
    %pad/u 5;
    %add;
    %load/vec4 v0x5c03858c7490_0;
    %pad/u 5;
    %add;
    %store/vec4 v0x5c0385923800_0, 0, 5;
    %load/vec4 v0x5c0385923800_0;
    %load/vec4 v0x5c0385924b40_0;
    %cmp/ne;
    %jmp/0xz  T_0.0, 4;
